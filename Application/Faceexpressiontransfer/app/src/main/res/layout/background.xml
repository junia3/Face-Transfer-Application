<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B6E8EA"
    android:backgroundTint="#B6E8EA">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="67dp"
        android:layout_height="74dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/yonsei2" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="107dp"
        android:layout_marginBottom="49dp"
        android:text="Team 4"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="107dp"
        android:layout_marginBottom="26dp"
        android:text="Face Transfer Application"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/scene_main"
        android:layout_width="235dp"
        android:layout_height="227dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/bg3"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/bg2"
        app:layout_constraintTop_toBottomOf="@+id/scene_main"
        app:srcCompat="@drawable/bg3"
        tools:srcCompat="@drawable/bg3" />

    <ImageView
        android:id="@+id/bg2"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="160dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bg3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/bg1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scene_main"
        app:srcCompat="@drawable/bg2"
        tools:srcCompat="@drawable/bg2" />

    <ImageView
        android:id="@+id/bg1"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintEnd_toStartOf="@+id/bg2"
        app:layout_constraintTop_toBottomOf="@+id/scene_main"
        app:srcCompat="@drawable/bg1"
        tools:srcCompat="@drawable/bg1" />

    <ImageView
        android:id="@+id/bg4"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintEnd_toStartOf="@+id/bg5"
        app:layout_constraintTop_toBottomOf="@+id/bg1"
        app:srcCompat="@drawable/bg4"
        tools:srcCompat="@drawable/bg4" />

    <ImageView
        android:id="@+id/bg5"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_marginStart="160dp"
        android:layout_marginEnd="156dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bg2"
        app:srcCompat="@drawable/bg5"
        tools:srcCompat="@drawable/bg5" />

    <ImageView
        android:id="@+id/bg6"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@+id/bg5"
        app:layout_constraintTop_toBottomOf="@+id/bg3"
        app:srcCompat="@drawable/bg6"
        tools:srcCompat="@drawable/bg6" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Choose Background"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bg5" />

</androidx.constraintlayout.widget.ConstraintLayout>